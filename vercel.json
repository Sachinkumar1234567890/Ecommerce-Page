{
    "builds" :[{
        "src": "ecommerce/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdasize": "15mb", "runtime": "python3.9"}},
        {
            "src": "build_files.sh",
            "use": "@vercel/static"
        }
    ],
    "routes":[
        {
            "src":"/staticfiles/(.*)",
            "dest":"/staticfiles/$1"
        },
        {
            "src":"/(.*)",
            "dest":"ecommerce/wsgi.py"
        }
    ]
}

